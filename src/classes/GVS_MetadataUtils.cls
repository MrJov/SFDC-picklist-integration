    public List<GVS_MetadataUtils.IReadResult> readMetadata(GVS_MetadataUtils.MetadataPort service, String metadataType, List<String> fullNames, Integer qtyToRead){
      Integer counter = 0;
      List<String> fullNamesToRead = new List<String>();
      List<GVS_MetadataUtils.IReadResult> results = new List<GVS_MetadataUtils.IReadResult>();
      while(counter < fullNames.size()){
        fullNamesToRead.add(fullNames.get(counter++));
        if(Math.mod(counter, qtyToRead) == 0 || counter == fullNames.size()){
          System.debug('readMetadata() > Reading: ' + fullNamesToRead);
          results.add(service.readMetadata(metadataType, fullNamesToRead));
          fullNamesToRead = new List<String>();
        }
      }
      return results;
    }

