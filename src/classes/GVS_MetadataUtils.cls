  public static String SOAP_M_URI = 'http://soap.sforce.com/2006/04/metadata';
  public static Integer CONNECTION_TIMEOUT = 120000;
  
  /**
  * Helper methos to create a connection to the Metadata API service
  **/
  public static GVS_MetadataUtils.MetadataPort createService(){
    GVS_MetadataUtils.MetadataPort service = new GVS_MetadataUtils.MetadataPort();
    service.SessionHeader = new GVS_MetadataUtils.SessionHeader_element();
    service.SessionHeader.sessionId = UserInfo.getSessionId();
    return service;
  }

  /**
  * Helper method to interpret a SaveResult, throws an exception if errors are found
  **/
  public static void handleSaveResults(List<GVS_MetadataUtils.SaveResult> saveResults){
    List<String> messages = new List<String>();
    for(GVS_MetadataUtils.SaveResult saveResult : saveResults){
      // Nothing to see?
      if(saveResult==null || saveResult.success) continue;
      // Construct error message and throw an exception
      else if(saveResult.errors!=null){
        messages.add((saveResult.errors.size() == 1 ? 'Error ' : 'Errors ') + 'occured processing component ' + saveResult.fullName + '.');
        for(GVS_MetadataUtils.Error error : saveResult.errors)
          messages.add(error.message + ' (' + error.statusCode + ').' + (error.fields != null && error.fields.size() > 0 ? ' Fields ' + String.join(error.fields, ',') + '.' : '' ));
        } 
        else if(!saveResult.success) messages.add('Request failed with no specified error.');
      }
    if(messages.size()>0) throw new GVS_MetadataUtilsException(String.join(messages, ' '));
  }
    
  public class GVS_MetadataUtilsException extends Exception { }
    
